<h1>All Shelters</h1>

<% @shelters.each do |shelter| %>
  <div id="shelter-<%= shelter.id %>">
    <h3><%= shelter.name %></h3>
  </div>
<% end %>

<div id="pending_apps">
<h3>Shelters with Pending Applications:</h3>
  <% @shelters_with_pending_applications.each do |shelter|%>
    <h3><%= shelter.name %></h3>
  <% end %>
</div>